          this.icon = data.icon\n          }\n          if (data.title) {\n            this.title = data.title\n          }\n\n          if (data.text) {\n            this.text = data.text\n          } else {\n            this.component = data.component\n            this.data = data.data\n            this.text = ''\n          }\n        } else {\n          this.component = false\n          this.text = data\n        }\n\n        return this.$refs.modal.open()\n      },\n      open: function (data) {\n          this.modal(data)\n      }\n    }\n  }\n</script>"]}]}